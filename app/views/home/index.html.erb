<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if user_signed_in? %>
  <%= link_to "ユーザーアウト", destroy_user_session_path, method: :delete %>
<% elsif client_signed_in? %>
  <%= link_to "新規投稿", new_job_path %>
  <%= link_to "クライアントアウト", destroy_client_session_path, method: :delete %>
<% else %>
  <%= link_to "ユーザー新規", new_user_registration_path %>
  <%= link_to "クライアント新規", new_client_registration_path %>
  <%= link_to "ユーザーイン", new_user_session_path %>
  <%= link_to "クライアントイン", new_client_session_path %>
<% end %>

<% if user_signed_in? %>
 <%= current_user.familyName + current_user.lastName %>
<% end %> 

<% if client_signed_in? %>
 <%= current_client.name %>
<% end %> 